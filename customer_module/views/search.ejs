<%- include header %>
	<h1>BOBO KA JC PAKYU</h1>
	<table>
		<% data.forEach(function(element) { %>
		<tr>
			<td> serviceName: </td>
			<td> <%=element.serviceName%> </td>
		</tr>

		<tr>
			<td>image:</td>
			<td><img src="data:image/png; base64, <%=new Buffer (element.image, 'binary').toString ('base64')%>"
				 alt="image"></td>
		</tr>
	<% }); %>
	</table>
<%- include footer %>